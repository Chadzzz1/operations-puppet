<VirtualHost *:80>
    ServerName os-reports.wikimedia.org
    DocumentRoot <%= @docroot %>

    ErrorLog /var/log/apache2/os-reports.wikimedia.org.error.log
    LogLevel warn
    CustomLog /var/log/apache2/os-reports.wikimedia.org.access.log wmf

    <Directory <%= @docroot %>>
        Require all granted
    </Directory>
</VirtualHost>
