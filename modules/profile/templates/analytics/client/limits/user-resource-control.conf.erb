[Slice]
# Slow down procs if they use more than this percentage of memory
MemoryHigh=60%
# If more than this percentage of memory is used, OOM killer will step in
MemoryMax=90%
# Users can't use swap memory
MemorySwapMax=0
# Each user can use max this % of one CPU
# From the docs: 'Use values > 100% for allotting CPU time on more than one CPU'
CPUQuota=<%= @cpu_quota %>%