<%- if @proxy_address and @proxy_address != '' -%>
http_proxy: "http://<%= @proxy_address %>:<%= @proxy_port %>"
<%- end -%>
seed_image: docker-registry.wikimedia.org/wikimedia-buster:latest
registry: <%= @registry %>
username: <%= @username %>
password: <%= @password %>
ca_bundle: /etc/ssl/certs/ca-certificates.crt
base_images:
  - docker-registry.wikimedia.org/wikimedia-stretch:latest
