# Note: This file is managed by puppet.

global
	maxconn 4096
	#debug
	#quiet
	user haproxy
	group haproxy
	stats socket <%= @socket %> mode 666 level user


defaults
	mode	http
	option	httplog
	option	dontlognull
	retries	3
	option redispatch
	maxconn	2000
	contimeout	50000
	clitimeout	500000
	srvtimeout	500000
