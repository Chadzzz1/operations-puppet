input {
  redis {
    host      => "<%= @host %>"
    port      => <%= @port %>
    data_type => "<%= @data_type %>"
    key       => "<%= @key %>"
<%- if @password -%>
    password  => "<%= @password %>"
<%- end -%>
    tags      => [ "redis" ]
<%- unless @plugin_id.empty? -%>
    id        => "<%= @plugin_id %>"
<%- end -%>
  }
}
