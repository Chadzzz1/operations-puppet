varnishtest "Clients are not allowed to specify X-Range"

server s1 {
    rxreq
    expect req.http.X-Range == <undef>
    expect req.http.X-Range-2 == "Yes"
    txresp
} -start

varnish v1 -arg "-p vcc_err_unref=false -p vcc_allow_inline_c=true -p cc_command='exec gcc -std=gnu99 -g -O2 -fstack-protector-strong -Wformat -Werror=format-security -Wall -pthread -fpic -shared -Wl,-x -o %o %s'" -vcl+backend {
    backend vtc_backend {
        .host = "${s1_addr}"; .port = "${s1_port}";
    }

    include "/usr/share/varnish/tests/wikimedia_upload-backend.vcl";
} -start

client c1 {
    txreq -hdr "Host: upload.wikimedia.org" -hdr "X-Range: No" -hdr "X-Range-2: Yes"
    rxresp
    expect resp.status == 200
}

client c1 -run
