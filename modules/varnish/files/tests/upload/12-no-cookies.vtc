varnishtest "Strip cookies (T137609)"

server s1 {
    rxreq
    txresp -hdr "Set-Cookie: foo" -hdr "Set-Cookie: bar"
} -start

varnish v1 -arg "-p vcc_err_unref=false -p vcc_allow_inline_c=true -p cc_command='exec gcc -std=gnu99 -g -O2 -fstack-protector-strong -Wformat -Werror=format-security -Wall -pthread -fpic -shared -Wl,-x -o %o %s'" -vcl+backend {
    backend vtc_backend {
        .host = "${s1_addr}"; .port = "${s1_port}";
    }

    include "/usr/share/varnish/tests/wikimedia_upload-frontend.vcl";
} -start

client c1 {
    txreq -url "/test" -hdr "X-Forwarded-Proto: https" -hdr "Host: upload.wikimedia.org"
    rxresp

    expect resp.status == 200
    expect resp.http.Set-Cookie == <undef>
} -run
