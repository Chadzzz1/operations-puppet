[keystone_authtoken]
auth_host = <%= @keystone_fqdn %>
auth_protocol = http
admin_user = novaadmin
admin_tenant_name = admin
www_authenticate_uri = http://<%= @keystone_fqdn %>:5000
auth_url = http://<%= @keystone_fqdn %>:35357
auth_type = password
project_domain_name = default
user_domain_name = default
project_name = admin
username = novaadmin
password = <%= @ldap_user_pass %>

[vendordata_context]
instance_domain = <%= @dhcp_domain %>
vendordata_file = /etc/novavendordata/vendordata.txt.jinja2
listen_port = <%= @vendordata_bind_port %>
