# HELP service_catalog_info State information for service::catalog
# TYPE service_catalog_info gauge
<% @info_by_service.sort.each do |service, info| -%>
service_catalog_info{instance="<%= service %>", state="<%= info['state'] %>"} 1
<% end %>
# HELP service_catalog_page Paging information for service::catalog
# TYPE service_catalog_page gauge
<% @info_by_service.sort.each do |service, info| -%>
service_catalog_page{instance="<%= service %>"} <%= info['page'] %>
<% end %>
