[Unit]
Description=Dumps misc rsyncer service
After=network.target

[Service]
User=root
ExecStart=/bin/bash /usr/local/bin/rsync-to-peers.sh --do_rsync_misc --do_rsync_miscsubs --miscdumpsdir <%= @miscdumpsdir %>  --miscremotedirs <%= @miscremotedirs %> --miscsubdirs <%= @miscsubdirs %> --miscremotesubs <%= @miscremotesubs %>

SyslogIdentifier=%N

[Install]
WantedBy=multi-user.target
