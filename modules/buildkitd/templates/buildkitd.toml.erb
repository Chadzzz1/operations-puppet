<% if @gckeepstorage > 0 -%>
[worker.oci]
gc = true
# MB units
gckeepstorage = <%= @gckeepstorage %>
<% end -%>

[dns]
# See https://github.com/moby/buildkit/blob/v0.10/cmd/buildkitd/config/config.go#L114
<%- if @nameservers.any? -%>
nameservers = <%= @nameservers.to_json %>
<%- end -%>
