service-name: isc-dhcp-server
dhcpd: /usr/sbin/dhcpd
dhcpd.conf: /etc/dhcp/dhcpd.conf
paths:
  - path: /etc/dhcp/automation/opt82-entries.ttyS0-115200
    file: /etc/dhcp/opt82-entries.ttyS0-115200
  - path: /etc/dhcp/automation/opt82-entries.ttyS1-115200
    file: /etc/dhcp/opt82-entries.ttyS1-115200
<% @mgmt_networks.each_key do |mgmt| -%>
  - path: /etc/dhcp/automation/mgmt-<%= mgmt %>/
    file: /etc/dhcp/automation/proxies/proxy-mgmt.<%= mgmt %>.conf
<% end -%>
